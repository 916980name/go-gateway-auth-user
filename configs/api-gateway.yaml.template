serverOptions:
  Addr: 0.0.0.0
  Runmode: debug
  Port: 8989
log:
  level: DEBUG
  # format: json
  output-paths: 
    - stdout
    - /tmp/api-gateway.log
db:
  redis:
    connectionString: redis://localhost:6379
caches:
  - name: cache-1
    type: mem
    max: 10
    defaulExpireMinute: 10
  - name: cache-1-blacklist
    type: mem
    max: 20
    defaulExpireMinute: 720
  - name: cache-2
    type: mem
    max: 30
    defaulExpireMinute: 10
  - name: cache-2-online
    type: redis
    max: 300
    defaulExpireMinute: 1440
  - name: cache-2-blacklist
    type: redis
    max: 3000
    defaulExpireMinute: 720
rateLimiters:
  - name: rl-1
    cacheName: cache-1
    max: 3
    refillInterval: 30
    refillNumber: 1
  - name: rl-1-blacklist
    cacheName: cache-1-blacklist
    max: 3
    refillInterval: 300
    refillNumber: 1
  - name: rl-2
    cacheName: cache-2
    max: 10
    refillInterval: 2
    refillNumber: 1
  - name: rl-2-login
    cacheName: cache-2-blacklist
    max: 3
    refillInterval: 2
    refillNumber: 1
  - name: rl-2-blacklist
    cacheName: cache-2-blacklist
    max: 3
    refillInterval: 300
    refillNumber: 1
sites:
  - hostname: file-transfer.local
    inOutFilter:
      limiterName: rl-1-blacklist
      loginPath: /trysignin
      logoutPath: /logout
      refreshTokenPath: /refreshToken
    routes:
      - path: /refreshToken
        method: GET
      - path: /logout
        method: GET, POST
        route: 127.0.0.1:8090
        privilege: P_USER
      - path: /trysignin
        method: POST
        route: 127.0.0.1:8090
        rateLimiter: 
          limiterName: rl-1
          limitType: limiterIP
      - path: /ls/**
        route: 127.0.0.1:8090
      - path: /msg**
        route: 127.0.0.1:8090
        privilege: P_USER
      - path: /ms/**
        route: 127.0.0.1:8090
      - path: /share/**
        route: 127.0.0.1:8090
        privilege: P_USER
      - path: /file**
        route: 127.0.0.1:8090
        privilege: P_USER
